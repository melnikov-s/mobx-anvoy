{
  "parser": "babel-eslint",
  "rules": {
    "strict": 0,
    "semi": 1,
    "no-unused-vars": [2, {"vars": "all", "args": "none"}],
    "no-undef": 1,
    "no-var": 1,
    "prefer-spread": 1,
    "object-shorthand": [1, "methods"],
    "no-this-before-super": 1,
    "constructor-super": 1,
    "no-const-assign": 1,
    "no-delete-var": 1,
    "no-shadow-restricted-names": 1,
    "no-undef-init": 1,
    "no-eval": 1,
    "dot-notation": 1,
    "no-caller": 1,
    "no-else-return": 1,
    "no-fallthrough": 1,
    "no-floating-decimal": 1,
    "no-iterator": 1,
    "no-labels": 1,
    "no-lone-blocks": 1,
    "no-native-reassign": 1,
    "no-new-wrappers": 1,
    "no-proto": 1,
    "yoda": 0,
    "eqeqeq": [2, "allow-null"],
    "no-empty": 2,
    "brace-style": 2,
    "no-with": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-multiple-empty-lines": 2,
    "no-multi-str": 2,
    "no-multi-spaces": 2,
    "padded-blocks": [
      2,
      "never"
    ],
    "key-spacing": [
      2,
      {
        "beforeColon": false,
        "afterColon": true
      }
    ],
    "space-unary-ops": [
      2,
      {
        "words": false,
        "nonwords": false
      }
    ],
    "space-before-function-paren": [
      2,
      "never"
    ],
    "no-spaced-func": 2,
    "space-in-parens": [
      2,
      "never"
    ],
    "comma-dangle": [
      2,
      "never"
    ],
    "no-trailing-spaces": 2,
    "max-len": [
      2,
      105
    ],
    "camelcase": [
      2,
      {
        "properties": "never"
      }
    ],
    "comma-style": [
      2,
      "last"
    ],
    "wrap-iife": 2,
    "space-infix-ops": 2,
    "keyword-spacing": [
      2,
      {
        "overrides": {
          "else": {
            "before": true
          },
          "while": {
            "before": true
          },
          "catch": {
            "before": true
          }
        }
      }
    ],
    "space-before-blocks": [
      2,
      "always"
    ],
    "indent": [
      2,
      4,
      {
        "SwitchCase": 1
      }
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "one-var": [
      2,
      "never"
    ],
    "comma-spacing": [
      2,
      {
        "before": false,
        "after": true
      }
    ],
    "semi-spacing": [
      2,
      {
        "before": false,
        "after": true
      }
    ]
  },
  "env": {
    "browser": true
  },
  "globals": {
    "it": true,
    "xit": true,
    "jasmine": true,
    "expect": true,
    "spyOn": true,
    "describe": true,
    "beforeEach": true,
    "afterEach": true
  }
}
